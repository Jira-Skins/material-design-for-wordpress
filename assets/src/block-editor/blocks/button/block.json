{
	"name": "material/button",
	"category": "material",
	"attributes": {
	   "label": {
		  "type": "string",
		  "source": "html",
		  "selector": ".mdc-button__label",
		  "default": ""
	   },
	   "type": {
		  "type": "string",
		  "default": "text"
	   },
	   "style": {
		  "type": "string",
		  "default": "text"
	   },
	   "iconPosition": {
		  "type": "string",
		  "default": "leading"
	   },
	   "cornerRadius": {
		  "type": "number"
	   },
	   "url": {
		  "type": "string",
		  "source": "attribute",
		  "selector": "a",
		  "attribute": "href",
		  "default": ""
	   },
	   "rel": {
		  "type": "string",
		  "source": "attribute",
		  "selector": "a",
		  "attribute": "rel",
		  "default": ""
	   },
	   "linkTarget": {
		  "type": "string",
		  "source": "attribute",
		  "selector": "a",
		  "attribute": "target"
	   },
	   "icon": {
		  "type": "object"
	   },
	   "backgroundColor": {
		  "type": "string"
	   },
	   "textColor": {
		  "type": "string"
	   },
	   "isSubmit": {
		  "type": "boolean",
		  "default": false
	   }
	}
 }